{
  "name": "@tngraphql/mail",
  "version": "1.0.11-alpha",
  "description": "Mail provider for tngraphql framework and has support for common mailing services to send emails",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --verbose --coverage --runInBand",
    "dev": "ts-node-dev example/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -b && npm run copy:files",
    "copy:files": "copyfiles -u 0 \"src/**/*.stub\" \"src/**/*.edge\" dist"
  },
  "author": "Nguyenpl117,TNGraphQL",
  "license": "MIT",
  "keywords": [
    "tngraphql",
    "tngraphql-framework",
    "tngraphql-mail"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@poppinss/dev-utils": "^1.0.4",
    "@tngraphql/console": "^1.0.0",
    "@tngraphql/graphql": "^1.0.0",
    "@tngraphql/illuminate": "^1.1.3",
    "@tngraphql/route": "^1.0.1",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "aws-sdk": "^2.650.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "edge.js": "1.1.4",
    "express": "^4.17.1",
    "i18n": "^0.8.6",
    "jest": "^25.1.0",
    "knex": "^0.20.13",
    "mailgun-js": "^0.22.0",
    "mysql": "^2.18.1",
    "nodemailer-mailgun-transport": "^2.0.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "email-validator": "^2.0.4",
    "nodemailer": "^6.4.6"
  },
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tngraphql/mail.git"
  },
  "bugs": {
    "url": "https://github.com/tngraphql/mail/issues"
  },
  "homepage": "https://github.com/tngraphql/mail#readme"
}
